# Roboflow API Configuration
# Get your API key from: https://app.roboflow.com/settings/api

# Required: Your Roboflow API Key
VITE_ROBOFLOW_API_KEY=YOUR_API_KEY_HERE

# Optional: Model Configuration (defaults shown below)
VITE_ROBOFLOW_MODEL_ID=deteksiparkirkosong
VITE_ROBOFLOW_MODEL_VERSION=6

# Note: After changing .env.local, restart your Vite dev server

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Backend API URLs
# Backend FastAPI server - all streams and ESP32 communication go through here (indirect connection)
VITE_BACKEND_URL=http://localhost:8069

